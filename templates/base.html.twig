    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://cdn.tailwindcss.com"></script>
{% endblock %}
<html>
<head>{% block head %}{% endblock %}
    <title>{% block title %}{% endblock %}</title>
